{
    "namespace": "Ext",

    "name": "weather-icons",
    "type": "code",

    "framework": "ext",
    "toolkit": "classic",
    "theme": "theme-triton",
    //"requires": [
    //    ...
    //],

    "creator": "RichardStyles",
    "summary": "Package wrapper for Weather Fonts",
    "detailedDescription": "Sencha Cmd package wrapper for Weather Fonts (https://erikflowers.github.io/weather-icons/)",

    "version": "1.0.5",
    "compatVersion": "1.0.0",
    "format": "1",

    "output": "${framework.dir}/build/packages/${package.name}",
    "local": true,

    "resource": {
        "paths": ""
    },

    "resources": [
        {
            "path": "${package.dir}/resources",
            "output": "shared"
        }
    ],

    "sass": {
        "namespace": "Ext",

        "etc": "${package.dir}/sass/etc/all.scss",

        "var": "${package.dir}/sass/var",

        "src": [
            "${package.dir}/sass/src",
            "${package.dir}/sass/src/all.scss"
        ]
    },

    "classpath": "${package.dir}/src",

    "overrides": "${package.dir}/overrides",

    "slicer": null
}
